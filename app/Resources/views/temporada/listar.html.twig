{% extends 'layout.html.twig' %}

{% block title %}Temporadas{% endblock %}

{% block section %}
    <div class="container">
        <h2>Temporadas</h2>
        <a href="{{ path('principal') }}" class="btn btn-default">
            <i class="fa fa-home"></i> Ir a principal
        </a>
        <ul>
            {% for temporada in temporadas %}
                <li>{{ temporada }}</li>
            {% endfor %}
        </ul>
    </div>
{% endblock %}